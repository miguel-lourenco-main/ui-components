{
  "id": "toggle",
  "name": "Toggle",
  "category": "form",
  "description": "A two-state button that can be either on or off. Commonly used for formatting controls and settings",
  "props": [
    {
      "name": "pressed",
      "type": "boolean",
      "required": false,
      "description": "The controlled pressed state of the toggle"
    },
    {
      "name": "onPressedChange",
      "type": "function",
      "required": false,
      "description": "Event handler called when the pressed state changes",
      "functionSignature": {
        "params": "pressed: boolean",
        "returnType": "void"
      }
    },
    {
      "name": "disabled",
      "type": "boolean",
      "required": false,
      "defaultValue": false,
      "description": "Whether the toggle is disabled"
    },
    {
      "name": "variant",
      "type": "enum",
      "required": false,
      "defaultValue": "default",
      "options": ["default", "outline"],
      "description": "The visual variant of the toggle"
    },
    {
      "name": "size",
      "type": "enum",
      "required": false,
      "defaultValue": "default",
      "options": ["sm", "default", "lg"],
      "description": "The size of the toggle"
    },
    {
      "name": "children",
      "type": "React.ReactNode",
      "required": false,
      "description": "The content to display inside the toggle"
    },
    {
      "name": "aria-label",
      "type": "string",
      "required": false,
      "description": "Accessible label for the toggle"
    },
    {
      "name": "className",
      "type": "string",
      "required": false,
      "defaultValue": "",
      "description": "Additional CSS classes for styling"
    }
  ],
  "tags": ["toggle", "button", "state", "form", "control"],
  "version": "1.0.0",
  "author": "UI Components Team"
} 